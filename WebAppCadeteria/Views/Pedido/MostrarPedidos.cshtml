@model List<Cadeteria.Model.Pedido>


<h1 class="text-center">Lista de Pedidos</h1>

<table class="table table-dark">
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Estado</th>
        <th scope="col">Observacion</th>
        <th>Cadete</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Estado</td>
            <td>@item.Observacion</td>
            <td>@item.Observacion</td>
        </tr>
    }
</table>
